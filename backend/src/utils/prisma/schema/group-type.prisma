model GroupType {
    id          String     @id @default(uuid(7))
    type        StudyType
    price       Decimal
    discount    Decimal?
    packageId   String  @map("package_id")

    bimbelPackage BimbelPackage @relation(fields: [packageId], references: [id])

    order Order []

    @@map("group_types")
}

enum StudyType {
  privat
  grup2
  grup3
  grup4
  grup5
  kelas
}